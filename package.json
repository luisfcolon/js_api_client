{
  "name": "js-api-client",
  "version": "1.0.7",
  "description": "Javascript api client wrapper for Axios",
  "main": "dist",
  "scripts": {
    "build": "./node_modules/.bin/babel --presets es2015 ./src/client.js -o ./dist/index.js",
    "test": "./node_modules/jest/bin/jest.js --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisfcolon/js_api_client.git"
  },
  "keywords": [
    "javascript",
    "api",
    "client",
    "axios",
    "wrapper"
  ],
  "author": "Luis Colon <luiscolon62@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luisfcolon/js_api_client/issues"
  },
  "homepage": "https://github.com/luisfcolon/js_api_client#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-uglifyjs": "^0.6.2",
    "jest": "^21.1.0",
    "moxios": "^0.4.0",
    "ohas": "^1.0.4",
    "sinon": "^10.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!dist/**/*.{js,jsx}"
    ]
  }
}
